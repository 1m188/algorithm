{
    // leetcode
    // 用户配置里的 workspace 应指向 leetcode 目录
    "leetcode-problem-rating.filePath": {
        "default": {
            "folder": "",
            "filename": "${id}.${kebab-case-name}.${ext}"
        },
        "golang": {
            "folder": "LC${id}",
            "filename": "LC${id}.${ext}"
        },
        "rust": {
            "folder": "../src/bin/",
            "filename": "lc-${id}-${kebab-case-name}.${ext}"
        }
    },
    "leetcode-problem-rating.includeTemplates": [
        {
            "language": "cpp",
            "template": [
                "using namespace std;",
            ]
        },
        {
            "language": "rust",
            "template": [
                "#![no_main]",
                "#![allow(dead_code)]",
                "struct Solution {}",
            ]
        }
    ],
}